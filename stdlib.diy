;; Some logical operators.

(define not
    (lambda (b)
        (if b #f #t)))

(define or
    (lambda (a b)
        (if (eq #t a)
            #t
            (if (eq #t b)
                #t
                #f))))

(define and
    (lambda (a b)
        (if (eq #t a)
            (if (eq #t b)
                #t
                #f)
            #f)))

(define xor
    (lambda (a b)
        (if (eq #t a)
            (if (eq #f b)
                #t
                #f)
            (if (eq #t b)
                #t
                #f))))



